{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ng-dompurify": {
      "projectType": "library",
      "root": "projects/ng-dompurify",
      "sourceRoot": "projects/ng-dompurify/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/ng-dompurify/tsconfig.lib.json",
            "project": "projects/ng-dompurify/ng-package.json"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ng-dompurify/src/test.ts",
            "tsConfig": "projects/ng-dompurify/tsconfig.spec.json",
            "karmaConfig": "projects/ng-dompurify/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-dompurify/tsconfig.lib.json",
              "projects/ng-dompurify/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "demo": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/demo",
      "sourceRoot": "projects/demo/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/demo",
            "index": "projects/demo/src/index.html",
            "main": "projects/demo/src/main.ts",
            "polyfills": "projects/demo/src/polyfills.ts",
            "tsConfig": "projects/demo/tsconfig.json",
            "aot": false,
            "assets": [],
            "styles": [
              "projects/demo/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "serve": {
              "tsConfig": "tsconfig.json"
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build:serve"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/demo/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-with-tests": {
      "root": "projects/app-with-tests/",
      "sourceRoot": "projects/app-with-tests/src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-with-tests",
            "index": "projects/app-with-tests/src/index.html",
            "main": "projects/app-with-tests/src/main.ts",
            "polyfills": "projects/app-with-tests/src/polyfills.ts",
            "tsConfig": "projects/app-with-tests/tsconfig.app.json",
            "assets": [
              "projects/app-with-tests/src/favicon.ico",
              "projects/app-with-tests/src/assets"
            ],
            "styles": [
              "projects/app-with-tests/src/styles.css"
            ],
            "scripts": [],
            "es5BrowserSupport": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/app-with-tests/src/environments/environment.ts",
                  "with": "projects/app-with-tests/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-with-tests:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-with-tests:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-with-tests:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/app-with-tests/src/test.ts",
            "polyfills": "projects/app-with-tests/src/polyfills.ts",
            "tsConfig": "projects/app-with-tests/tsconfig.spec.json",
            "karmaConfig": "projects/app-with-tests/karma.conf.js",
            "styles": [
              "projects/app-with-tests/src/styles.css"
            ],
            "scripts": [],
            "assets": [
              "projects/app-with-tests/src/favicon.ico",
              "projects/app-with-tests/src/assets"
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/app-with-tests/tsconfig.app.json",
              "projects/app-with-tests/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app-with-tests-e2e": {
      "root": "projects/app-with-tests-e2e/",
      "projectType": "application",
      "prefix": "",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/app-with-tests-e2e/protractor.conf.js",
            "devServerTarget": "app-with-tests:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-with-tests:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": "projects/app-with-tests-e2e/tsconfig.e2e.json",
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "ng-dompurify"
}